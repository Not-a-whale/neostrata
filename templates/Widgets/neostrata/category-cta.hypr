<div class="category-cta {{model.config.system}}">
  <h4 class="slashHeading"><span>{{model.config.title}}</span></h4>
  <div>
    <img class="heroImage" src="{{model.config.imagePath}}"/>
    <div class="body">
      <div class="description">{{model.config.description}}
      <div class="cta"><a class="primary-btn" href="{{model.config.linkPath}}">{{model.config.linkTitle}}</a></div>
    </div>
  </div>
</div>
