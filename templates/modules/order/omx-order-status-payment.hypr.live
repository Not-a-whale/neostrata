{% if model.$.type == 1 %}
		<div class="mz-payment-type"> {{ model.CreditCardName|first }}</div>
		<div class="mz-payment-number"> {{labels.orderStatusPaymentCCEnding|string_format(model.omxCCNumber) }}</div>
		<div class="mz-payment-type"> {{labels.orderStatusPaymentCCExpires|string_format(model.CreditCardExpDate|first|date("m/Y")) }}</div>
{% else  %}
	{% if model.$.type == 13 %}
		<div class="mz-payment-type"> {{ model.$.description }}</div>

	{% endif %}
{% endif %}
